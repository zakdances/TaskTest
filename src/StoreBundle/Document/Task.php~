<?php
// src/StoreBundle/Document/Task.php
namespace StoreBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Task
{
    /**
     * @MongoDB\Id
     */
     protected $id;
     
    /**
    * @MongoDB\Field(type="string")
    */
    protected $label;

    /**
    * @MongoDB\Field(type="string")
    */
    protected $status;

    /**
     * Get id
     *
     * @return id $id
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set label
     *
     * @param string $label
     * @return $this
     */
    public function setLabel($label)
    {
        $this->label = $label;
        return $this;
    }

    /**
     * Get label
     *
     * @return string $label
     */
    public function getLabel()
    {
        return $this->label;
    }
}
